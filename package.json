{
  "name": "geomag-algorithms",
  "version": "1.1.1",
  "homepage": "http://geomag.usgs.gov/",
  "repository": "https://github.com/usgs/geomag-algorithms.git",
  "description": "Geomagnetism algorithms.",
  "author": {
    "name": "Jeremy Fee",
    "email": "jmfee@usgs.gov"
  },
  "contributors": [
    {
      "name": "Abram Claycomb",
      "email": "aclaycomb@usgs.gov"
    }
  ],
  "keywords": [
    "usgs",
    "geomag",
    "hazdev"
  ],
  "scripts": {
    "clean": "rimraf '**/*.pyc'",
    "coverage": "pytest --cov=geomagio --cov-report xml",
    "lint": "flake8 && echo \"No linting errors found\"",
    "test": "pytest test",
    "watch": "npm-watch"
  },
  "watch": {
    "lint": {
      "extensions": "py",
      "patterns": [
        "bin",
        "geomagio",
        "test"
      ]
    },
    "test": {
      "extensions": "py",
      "patterns": [
        "bin",
        "geomagio",
        "test"
      ]
    }
  },
  "license": "Public Domain",
  "dependencies": {},
  "devDependencies": {
    "npm-watch": "^0.3.0",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": ">=4"
  }
}
